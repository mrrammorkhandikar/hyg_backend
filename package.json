{
  "name": "drbushramirzah-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "NODE_ENV=production node dist/server.js",
    "build": "tsc",
    "seed": "ts-node src/seed/seedAdmin.ts",
    "prod": "npm run build && npm start",
    "render-build": "npm install && npm run build",
    "render-start": "npm start",
    "docker:build": "docker build -t drbushramirzah-backend .",
    "docker:run": "docker run -p 8080:8080 --env-file .env drbushramirzah-backend",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.25.0",
    "@types/open": "^6.1.0",
    "axios": "^1.13.1",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "googleapis": "^169.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.12",
    "open": "^11.0.0",
    "pg": "^8.16.3",
    "slugify": "^1.6.6",
    "tslib": "^2.8.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^7.0.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
